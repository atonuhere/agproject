<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/layouts">
    <excludes>
       <pattern>/nodecorate/*</pattern>
        <pattern>/css/*</pattern>
        <pattern>/js/*</pattern>
        <pattern>/images/*</pattern>
        <pattern>/web/ajax/*</pattern>
    </excludes>

     <decorator name="indexWide" page="layoutWide.jsp">
        <pattern>/index2.jsp</pattern>
    </decorator>
    
    <decorator name="index" page="layoutBoxed.jsp">
        <pattern>/index.jsp</pattern>
        <pattern>/home.jsp</pattern>
        <pattern>/login.jsp</pattern>
       
    </decorator>
       
    <decorator name="onecolumn" page="onecolumnLayout.jsp">
     	<pattern>/error.jsp</pattern>
        <pattern>/error404.jsp</pattern>
     	<pattern>/web/home.jsp</pattern>
    </decorator>
    <decorator name="secureColumn" page="twocolumnLayout.jsp">
     	<pattern>/web/incident/*</pattern>
     	<pattern>/web/master/*</pattern>
    </decorator>
</decorators>
