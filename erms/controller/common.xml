<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
   "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="common" namespace="/common" extends="default">

		<action name="home" class="homeAction" method="execute">
			<result name="success">/web/home.jsp</result>
		</action>

		<action name="register" class="userAction" method="execute">
			<result type="redirect">/</result>
			<result name="input">/web/register.jsp</result>
		</action>
       
        
	</package>
</struts>		